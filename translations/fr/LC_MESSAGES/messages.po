msgstr "hébergez votre site web"

#: index.html:243
msgid "share files in common (e.g. with Nextcloud)"
msgstr "partagez des fichiers en commun (par ex. avec Nextcloud)"

#: index.html:244
msgid "edit multiple documents in real time"
msgstr "éditez des documents à plusieurs en temps réel"

#: index.html:245
msgid "manage your accounting"
msgstr "gérez votre compta"

#: index.html:246
msgid "set up a team chat"
msgstr "mettez en place un chat d'équipe"

#: index.html:252
