msgid "Examples of uses"
msgstr "Exemples d’usatges"

#: index.html:326
msgid "For you and your friends"
msgstr "Per vos e vòstres amics"

#: index.html:329
msgid "synchronize your calendars and contacts"
msgstr "sincronizar vòstres agendas e contactes"

#: index.html:330
msgid "keep your passwords and notes safe"
msgstr "gardar vòstres senhals e vòstras nòtas en seguretat"

#: index.html:331
msgid "host mailboxes"
msgstr "albergar de bóstias"

#: index.html:332
msgid "organize your photos, videos and music"
msgstr "organizar vòstras fòtos, vidèos e musica"

#: index.html:333
msgid "join alternative social media"
msgstr "rejónher los malhums socials alternatius"

#: index.html:339
